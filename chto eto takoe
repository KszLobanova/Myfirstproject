stroka = input()
counter = 1
letter_index = 0
following_letter_index = letter_index + 1  # aaabbnnnw
for letter in stroka:
    if letter == stroka[following_letter_index]:
        counter = counter + 1
        finalstroka = letter + str(counter)
    else:
        counter = counter
        finalstroka = letter + str(counter)
    if counter == 1:
        finalstroka = letter
print(finalstroka)
